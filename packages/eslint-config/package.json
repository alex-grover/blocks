{
  "name": "@alex-grover/eslint-config",
  "description": "A shareable ESLint configuration.",
  "author": "Alex Grover <hello@alexgrover.me> (https://alexgrover.me)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex-grover/blocks.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "version": "0.0.12",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint --cache --cache-location node_modules/.cache --config index.ts",
    "check:lint": "bun run --bun lint .",
    "fix:lint": "bun run check:lint --fix",
    "check:types": "tsc"
  },
  "peerDependencies": {
    "@eslint/js": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": ">=9.0.0",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-unused-imports": "*",
    "typescript-eslint": "*"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.6"
  }
}
