{
  "name": "@alex-grover/stylelint-config",
  "version": "0.0.1",
  "description": "A shareable Stylelint configuration.",
  "main": "index.json",
  "author": "Alex Grover <hello@alexgrover.me> (https://alexgrover.me)",
  "license": "ISC",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "scripts": {
    "check:format": "prettier --check .",
    "fix:format": "prettier --write ."
  },
  "devDependencies": {
    "@alex-grover/prettier-config": "workspace:^",
    "prettier": "3.0.1"
  },
  "peerDependencies": {
    "stylelint": "*",
    "stylelint-config-css-modules": "*",
    "stylelint-config-recess-order": "*",
    "stylelint-config-standard": "*"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "prettier": "@alex-grover/prettier-config"
}
